  "type": "module",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "nest build",
    "start": "dotenv -c -- nest start",
    "start:dev": "dotenv -c -- nest start --watch",
    "start:swc": "dotenv -c -- nest start --watch -b swc --type-check",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:reset": "dotenv -c -- prisma migrate reset",
    "prisma:migrate": "dotenv -c -- prisma migrate dev",
    "prisma:migrate:create": "dotenv -c -- prisma migrate dev --create-only",
    "prisma:push": "dotenv -c -- prisma db push",
    "prisma:seed": "dotenv -c -- prisma db seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "svc:main:up": "docker compose -f dev-utils/docker-compose.dev-svc.yml -f dev-utils/docker-compose.main.yml up -d --build -t 1",
    "svc:dev:up": "docker compose -f dev-utils/docker-compose.dev-svc.yml up -d --force-recreate -t 1",
    "svc:dev:down": "docker compose -f dev-utils/docker-compose.dev-svc.yml down",
    "db:dev:backup": "docker exec -i patent-parser-db-dev sh /opt/backups/@backup.sh",
    "db:dev:restore": "docker exec -i patent-parser-db-dev sh /opt/backups/@restore.sh",
    "ch:migrate": "dotenv -c -- tsx migrations-clickhouse.ts"
  },
