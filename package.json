{
  "name": "patent-parser",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "nest build",
    "start:dev": "dotenv -c -- nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:reset": "dotenv -c -- prisma migrate reset",
    "prisma:migrate": "dotenv -c -- prisma migrate dev",
    "prisma:migrate:create": "dotenv -c -- prisma migrate dev --create-only",
    "prisma:push": "dotenv -c -- prisma db push",
    "prisma:seed": "dotenv -c -- prisma db seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "svc:main:up": "docker compose -f dev-utils/docker-compose.dev-svc.yml -f dev-utils/docker-compose.main.yml up -d --build -t 1",
    "svc:dev:up": "docker compose -f dev-utils/docker-compose.dev-svc.yml up -d --force-recreate -t 1",
    "svc:dev:down": "docker compose -f dev-utils/docker-compose.dev-svc.yml down",
    "db:dev:backup": "docker exec -i patent-parser-db-dev sh /opt/backups/@backup.sh",
    "db:dev:restore": "docker exec -i patent-parser-db-dev sh /opt/backups/@restore.sh",
    "ch:migrate": "dotenv -c -- tsx migrations-clickhouse.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@clickhouse/client": "^0.3.0",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/schedule": "^4.0.1",
    "@paljs/plugins": "^6.0.7",
    "@prisma/client": "5.10.2",
    "axios": "^1.6.7",
    "class-transformer": "^0.5.1",
    "csv-stringify": "^6.4.6",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.5",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.12.2",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "node-html-parser": "^6.1.12",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "prisma-graphql-type-decimal": "^3.0.0",
    "prisma-nestjs-graphql": "^19.2.0",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "socks-proxy-agent": "^8.0.2",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.25",
    "@types/papaparse": "^5.3.14",
    "@types/pg": "^8.11.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "prisma": "^5.10.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.12.0",
    "typescript": "^5.4.2"
  }
