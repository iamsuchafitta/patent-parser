{
  "name": "patent-parser",
  "version": "0.0.1",
  "description": "",
  "author": "Kulinchenko Inna Alexandrovna",
  "type": "module",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "build": "nest build",
    "start": "dotenv -c -- nest start",
    "start:dev": "dotenv -c -- nest start --watch",
    "start:swc": "dotenv -c -- nest start --watch -b swc --type-check",
    "start:bun": "bun --watch src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:reset": "dotenv -c -- prisma migrate reset",
    "prisma:migrate": "dotenv -c -- prisma migrate dev",
    "prisma:migrate:create": "dotenv -c -- prisma migrate dev --create-only",
    "prisma:push": "dotenv -c -- prisma db push",
    "prisma:seed": "dotenv -c -- prisma db seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "svc:main:up": "docker compose -f dev-utils/docker-compose.dev-svc.yml -f dev-utils/docker-compose.main.yml up -d --build -t 1",
    "svc:dev:up": "docker compose -f dev-utils/docker-compose.dev-svc.yml up -d --force-recreate -t 1",
    "svc:dev:down": "docker compose -f dev-utils/docker-compose.dev-svc.yml down",
    "db:dev:backup": "docker exec -i patent-parser-db-dev sh /opt/backups/@backup.sh",
    "db:dev:restore": "docker exec -i patent-parser-db-dev sh /opt/backups/@restore.sh",
    "clickhouse:migrate": "dotenv -c -- tsx clickhouse/main.ts"
  },
  "dependencies": {
    "@apollo/server": "4.10.4",
    "@clickhouse/client": "1.0.2",
    "@lifeomic/attempt": "3.1.0",
    "@nestjs/apollo": "12.1.0",
    "@nestjs/common": "10.3.8",
    "@nestjs/core": "10.3.8",
    "@nestjs/graphql": "12.1.1",
    "@nestjs/platform-express": "10.3.8",
    "@nestjs/schedule": "4.0.2",
    "@prisma/client": "5.14.0",
    "async-mutex": "0.5.0",
    "async-retry": "1.3.3",
    "axios": "1.7.2",
    "csv-parser": "3.0.0",
    "dayjs": "1.11.11",
    "fast-equals": "5.0.1",
    "graphql": "16.8.1",
    "graphql-scalars": "1.23.0",
    "graphql-type-json": "0.3.2",
    "joi": "17.13.1",
    "lodash-es": "4.17.21",
    "lodash-omitdeep": "1.3.5",
    "ms": "2.1.3",
    "neat-csv": "7.0.0",
    "node-html-parser": "6.1.13",
    "p-map": "7.0.2",
    "p-retry": "6.2.0",
    "papaparse": "5.4.1",
    "pg": "8.11.5",
    "puppeteer": "22.10.0",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "reflect-metadata": "0.2.2",
    "socks-proxy-agent": "8.0.3",
    "user-agents": "1.1.214"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.5.7",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/async-retry": "1.4.8",
    "@types/express": "4.17.21",
    "@types/lodash-es": "4.17.12",
    "@types/ms": "0.7.34",
    "@types/node": "20.12.12",
    "@types/papaparse": "5.3.14",
    "@types/pg": "8.11.6",
    "@types/user-agents": "1.0.4",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "prisma": "5.14.0",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "type-fest": "4.18.2",
    "typescript": "5.4.5"
  },
  "trustedDependencies": [
    "@apollo/protobufjs",
    "@nestjs/core",
    "@prisma/client",
    "@prisma/engines",
    "bufferutil",
    "prisma",
    "puppeteer",
    "utf-8-validate"
  ]
}
